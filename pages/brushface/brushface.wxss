/* pages/brushface/brushface.wxss */
.box {
  width: 350px;
  height: 488px;
  margin: 0 auto;
  margin-top: 10px;
}

.fanhui {
  width: 100%;
  height: 25px;
}

.fanhui image {
  width: 25px;
  height: 25px;
  float: left;
}

.avatar {
  width: 100%;
  height: 45px;
  margin-top: 20px;
}

.avatar image {
  width: 45px;
  height: 100%;
  float: left;
  border-radius: 50px;
}

.name {
  width: 140px;
  height: 40px;
  line-height: 40px;
  color: rgb(16, 16, 16);
  font-size: 28px;
  text-align: left;
  margin-top: 15px;
}

.welcome {
  width: 208px;
  height: 17px;
  line-height: 17px;
  color: rgb(121, 121, 121);
  font-size: 12px;
  text-align: left;
  margin-top: 5px;
}

.shualian {
  width: 300px;
  height: 257px;
  background-color: rgba(255, 255, 255, 1);
  margin: 10px auto;
}

.shualian image {
  width: 100%;
  height: 100%;
}

.jieshao {
  width: 97px;
  height: 17px;
  line-height: 17px;
  color: rgb(121, 121, 121);
  font-size: 12px;
  margin: 0 auto;
}
.buttomLoginBox {
  width: 230px;
  height: 55px;
  margin: 0 auto;
  margin-top: 130px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.bl {
  width: 50px;
  height: 55px;
}
.bl image{
  width: 35px;
  height: 35px;
  margin-left: 7.5px;
}
.otherLoginText{
  font-size: 11px;
  text-align: center;
  font-weight: 600;
}