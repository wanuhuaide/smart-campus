/* pages/login/login.wxss */
.box {
  width: 350px;
  height: 488px;
  margin: 0 auto;
  margin-top: 10px;
}

.top {
  width: 100%;
  height: 25px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.topRight {
  font-weight: 600;
  line-height: 25px;
  font-size: 15px;
  font-family: "宋体";
}

.top image {
  width: 20px;
  height: 20px;
  margin-top: 2.5px;
}

.loginTitle {
  font-size: 25px;
  font-weight: 600;
  margin-top: 30px;
}

.loginBox {
  width: 100%;
  height: 110px;
  margin-top: 50px;
}

.loginBox input {
  border-bottom: 1px solid rgb(224, 220, 220);
  height: 45px;
  margin-top: 5px;
  font-size: 16px;
  color: rgb(0, 0, 0);
}

.placeholderColor {
  color: rgb(179, 174, 174);
  font-size: 14px;
  font-weight: 600;
}

.question {
  font-size: 13px;
  color: rgb(146, 145, 141);
  font-weight: 600;
}

.loginBtn {
  width: 90%;
  height: 40px;
  background-color: green;
  border-radius: 10px;
  margin: 0 auto;
  text-align: center;
  line-height: 40px;
  color: white;
  font-weight: 600;
  font-family: "宋体";
  margin-top: 35px;
  background: linear-gradient(-90deg, #00C9A7, #008F7A);
}

.buttomLoginBox {
  width: 230px;
  height: 55px;
  margin: 0 auto;
  margin-top: 250px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.bl {
  width: 50px;
  height: 55px;
}
.bl image{
  width: 35px;
  height: 35px;
  margin-left: 7.5px;
}
.otherLoginText{
  font-size: 11px;
  text-align: center;
  font-weight: 600;
}
