/* pages/found/found.wxss *//* pages/found/found.wxss */
.headBox {
  width: 100%;
  height: 44px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 2px solid #FEF7FF;
}

.headBoxTitle {
  height: 44px;
  width: 60px;
  font-size: 18px;
  font-weight: 600;
  text-align: center;
  line-height: 44px;
}

.searchBox {
  height: 25px;
  width: 200px;
  margin-top: 10px;
  border-radius: 10px;
  border: 1px solid #FEF7FF;
}

.searchBox image {
  height: 25px;
  width: 25px;
  float: left;
  margin-left: 5px;
}

.searchBox input {
  height: 25px;
  float: left;
  font-size: 12px;
  width: 160px;
  color: rgb(74, 75, 75);
  margin-left: 8px;
}

.headBoxRight {
  height: 100%;
  width: 100px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.headBoxRight image {
  width: 23px;
  height: 23px;
}
.box {
  width: 350px;
  height: 500px;
  margin: 0 auto;
  margin-top: 10px;
}
.banner{
  width: 100%;
  height: 130px;
  position: relative;
  margin-top: 20px;
}
.banner image{
  height: 100%;
  width: 100%;
  border-radius: 20px;
  position: absolute;
}
.bannerContent{
  width: 150px;
  height: 80px;
  position: absolute;
  left: 20px;
  color: white;
  font-family: "宋体";
  font-weight: 600;
  top: 20px;
}
.bannerTitle{
  font-size: 16px;
}
.bannerText{
  font-size: 10px;
  margin-top: 15px;
}
.menu{
  height: 30px;
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 25px;
}
.menu1{
  font-size: 14px;
  width: 70px;
  height: 30px;
  text-align: center;
  line-height: 48px;
  font-weight: 600;
}
#menuFirst{
  font-size: 14px;
  width: 90px;
  height: 30px;
  font-size: 20px;
  font-weight: 600;
  text-align: center;
  line-height: 40px;
  border-bottom: 2px solid greenyellow;
}
.announcementList{
  width: 100%;
  height: 220px;
  margin-top: 30px;
  font-family: "宋体";
}
.announcementList1{
  width: 100%;
  height: 60px;
  margin-top: 40px;
  position: relative;
  border-radius: 10px;
  box-shadow: 0 4px 4px rgba(0, 0, 0, .1);
}
.announcementListContent{
  width: 270px;
  height: 100%;
  position: absolute;
}
.announcementListContent1{
  font-weight: 600;
  font-size: 15px;
  margin-left: 15px;
  margin-top: 3px;
}
.announcementListContent2{
  font-size: 10px;
  color: rgb(130, 132, 133);
  margin-top: 10px;
  margin-left: 15px;
}
.announcementList1ImageBox{
  height: 55px;
  width: 55px;
  position: absolute;
  margin-left: 290px;
  background: linear-gradient(0deg, #00C9A7, #4FFBDF);
  border-radius: 10px;
  margin-top: 2.5px;
}
.announcementList1ImageBox image{
  width: 100%;
  height: 100%;
}
#announcementList1ImageBox2{
  background: linear-gradient(0deg, #D2E6EE, #24819C);
}

#announcementList1ImageBox3{
  background: linear-gradient(0deg, #4FFBDF, #008B74);
}